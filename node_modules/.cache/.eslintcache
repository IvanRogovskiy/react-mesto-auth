AvatarPopup.js",["198"],[],"import PopupWithForm from \"./PopupWithForm\";\nimport React, {useState} from \"react\";\n\nconst EditAvatarPopup = ({isOpened, onClose, onAvatarUpdate}) => {\n  // const inputRef = useRef();\n\n  const [link, setLink] = useState('');\n  const [isValid, setIsValid] = useState(true);\n  const [isBtnActive, setBtnActive] = useState(false);\n\n  const handleClose = () => {\n    setIsValid(true);\n    onClose();\n  }\n\n  const handleLinkChange = (e) => {\n    setLink(e.target.value);\n    if (e.target.validity.valid) {\n        setIsValid(true);\n        setBtnActive(true);\n    } else {\n        setIsValid(false);\n        setBtnActive(false);\n    }\n  }\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    onAvatarUpdate(link)\n  }\n\n  return (\n      <PopupWithForm\n          name='edit'\n          title='Редактировать аватар'\n          onClose={handleClose}\n          isOpened={isOpened}\n          onSubmit={handleSubmit}\n      >\n          <>\n              <label className=\"popup__field\">\n                  <input value={link} onChange={handleLinkChange}\n                         className={`popup__input popup__input_type_title ${isValid ? '' : 'popup__input_type_error'}`}\n                         type=\"url\"\n                         placeholder=\"Адрес картинки\" defaultValue={''}\n                         required/>\n                  <span className={`popup__input-error link-input-error ${isValid ? '' :  'popup__input-error_active'}`}>Please enter a url</span>\n                  <input type=\"submit\" className={`popup__save popup__save_avatar-update ${isBtnActive ? '' : 'popup__save_inactive'}`} value={'Сохранить'}/>\n              </label>\n          </>\n      </PopupWithForm>\n  )\n}\n\nexport default EditAvatarPopup",[],"/Users/ivanrogovskiy/Documents/tries/yandex/mesto-react/src/components/AddPlacePopup.js",[],[],"/Users/ivanrogovskiy/Documents/tries/yandex/react-mesto-auth/src/index.js",[],[],"/Users/ivanr